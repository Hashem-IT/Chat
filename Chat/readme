Docker:
    docker run -p 9000:9000 sapringapiopenjdk => bildet ein ducker
    docker ps => zeiget alle docker
    7606d2e3bbcb   sapringapiopenjdk   "java -jar Chat.jar"   7 seconds ago   Up 7 seconds   0.0.0.0:9000->9000/tcp

    docker stop 7606d2e3bbcb => stopt Docker und braucht name von docker
    docker start c561ef435ebd => start socker und braucht name von docker

    iamge bilden
      //befehl  bau      name    hier
        docker build -t chat-app .

      // image zu zeigen
        docker images

      //docker bauen
      // befehl run      port docker : host chat:continer chat-app: image
        docker run -d -p 9000:9000 --name=chat chat-app

daten bank:

    docker-compose.yml => erstellen mit details rein
    application.properties =>  erstellen mit details rein
    docker-compose up -d    => db starten , mit -d l√§ft docker in hinter, ohne bleibt in Terminal run





